{
  "name": "@hawk-hhg/hawki-client",
  "version": "1.1.3",
  "description": "A JS library that allows you to connect to the HAWKI backend through a shared interface.",
  "main": "./dist/hawki-client.umd.js",
  "module": "./dist/hawki-client.mjs",
  "types": "./dist/index.d.ts",
  "repository": "https://github.com/hawk-digital-environments/hawki-client",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "node_modules/.bin/vite build",
    "watch": "node_modules/.bin/vite build --watch"
  },
  "type": "module",
  "keywords": [
    "HAWK",
    "HAWKI"
  ],
  "files": [
    "dist",
    "docs",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/hawki-client.umd.js",
      "import": "./dist/hawki-client.es.js"
    }
  },
  "author": "HAWKI Team <ki@hawk.de>",
  "license": "Apache-2.0",
  "devDependencies": {
    "vite": "^7.1.11",
    "vite-plugin-dts": "^4.5.0"
  },
  "dependencies": {
    "dexie": "^4.2.1",
    "laravel-echo": "^2.2.4",
    "pusher-js": "^8.4.0"
  }
}
